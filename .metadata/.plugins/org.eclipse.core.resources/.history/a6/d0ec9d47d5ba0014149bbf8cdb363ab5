#include <msp430.h>
#include "Mem_Manager.h"

/*
 * main.c
 */
int main(void) {
    WDTCTL = WDTPW | WDTHOLD;	// Stop watchdog timer

    osal_mem_init();
    //osal_mem_kick();
    char *pBuffer = (char*) osal_mem_alloc(200);
   int i  = 0;
   for(;i< 200;i++)
    pBuffer[i] = 'B';

     osal_mem_free( pBuffer);
      *pBuffer = (char*) osal_mem_alloc(200);
      int i  = 0;
      for(;i< 200;i++)
       pBuffer[i] = 'B';

while(1);


	return 0;
}
